  "paths": {
    "crypto": [
      "node_modules/crypto-browserify"
    ],
    "stream": [
      "node_modules/stream-browserify"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "prettier",
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:cypress/recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/cypress/**/*.js",
          "**/__mocks__/**/*.js",
          "**/__tests__/**/*.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
