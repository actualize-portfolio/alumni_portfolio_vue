    "axios": "^0.23.0",
    "bootstrap": "^5.2.0",
    "chart.js": "^3.8.1",
    "core-js": "^3.24.1",
    "crypto-browserify": "^3.12.0",
    "datamaps": "^0.5.9",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "pluralize": "^8.0.0",
    "startbootstrap-resume": "^7.0.4",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4",
    "velocity-animate": "^1.5.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
