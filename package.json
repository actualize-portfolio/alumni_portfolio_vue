    "cypress": "^9.1.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^7.0.0",
    "flush-promises": "^1.0.2",
    "prettier": "^2.5.1",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
